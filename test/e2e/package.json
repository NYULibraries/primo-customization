    "test:e2e:dev:updatesnapshots": "yarn test:e2e:dev --update-snapshots",
    "test:e2e:prod": "yarn test:e2e:prod:clean && VIEW=01NYU_INST-NYU yarn playwright test",
    "test:e2e:prod:clean": "rm -rf tests/actual/01NYU_INST-NYU/* && rm -rf tests/diffs/01NYU_INST-NYU/*",
    "test:e2e:prod:updatesnapshots": "yarn test:e2e:prod --update-snapshots"
  },
  "dependencies": {
    "@playwright/test": "1.37",
    "dotenv": "16.0.3",
    "js-beautify": "1.14.7"
  }
}
